def reverse_complement(dna):
    dna_r = dna[::-1]
    rna = ''
    for symbol in dna_r:
        if symbol == 'C':
            rna = rna + 'G'
        elif symbol == 'G':
            rna = rna + 'C'
        elif symbol == 'A':
            rna = rna + 'T'
        elif symbol == 'T':
            rna = rna + 'A'
